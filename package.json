{
  "name": "nextbooking-angular2",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "xi18n": "ng xi18n --output-path src/locale && xliffmerge --profile xliffmerge.json",
    "serve:da": "ng serve --aot --i18n-file=src/locale/messages.da.xlf --locale=da --i18n-format=xlf",
    "serve:en": "ng serve --aot --i18n-file=src/locale/messages.en.xlf --locale=en --i18n-format=xlf",
    "build:da": "ng build --output-path=dist/da --aot --bh /da/ --i18n-file=src/locale/messages.da.xlf --i18n-format=xlf --locale=da",
    "build:en": "ng build --output-path=dist/en --aot --bh /en/ --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --locale=en"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/compiler-cli": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/platform-server": "^4.1.3",
    "@angular/router": "^4.1.3",
    "@types/fullcalendar": "^2.7.42",
    "@types/jquery.datatables": "^1.10.36",
    "angular2-moment": "^1.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.4",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "extract-text-webpack-plugin": "^2.1.2",
  "jasmine-core": "2.5.2",
    "karma": "1.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.31",
    "ngx-i18nsupport": "^0.5.0",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "^4.0.2",
    "typescript": "2.3.3",
    "webdriver-manager": "10.2.5",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^1.9.0"
  }
}
